<!DOCTYPE web-app PUBLIC
 "-//Sun Microsystems, Inc.//DTD Web Application 2.3//EN"
 "http://java.sun.com/dtd/web-app_2_3.dtd" >

<web-app>

  <display-name>Archetype Created Web Application</display-name>

  <servlet>
    <servlet-name>CssServlet</servlet-name>
    <jsp-file>/css/table.css</jsp-file>
  </servlet>

  <servlet>
    <servlet-name>GetMyLocationServlet</servlet-name>
    <jsp-file>/js/getMyLocation.js</jsp-file>
  </servlet>

  <servlet>
    <servlet-name>GetWifiDataServlet</servlet-name>
    <jsp-file>/js/getWifiData.js</jsp-file>
  </servlet>

  <servlet>
    <servlet-name>UpdateHistoryServlet</servlet-name>
    <jsp-file>/js/updateHistory.js</jsp-file>
  </servlet>

  <servlet>
    <servlet-name>BookmarkServlet</servlet-name>
    <jsp-file>/js/bookmark.js</jsp-file>
  </servlet>

  <servlet-mapping>
    <servlet-name>CssServlet</servlet-name>
    <url-pattern>/css/*</url-pattern>
  </servlet-mapping>

  <servlet-mapping>
    <servlet-name>GetMyLocationServlet</servlet-name>
    <url-pattern>/js/getMyLocation.js</url-pattern>
  </servlet-mapping>

  <servlet-mapping>
    <servlet-name>GetWifiDataServlet</servlet-name>
    <url-pattern>/js/getWifiData.js</url-pattern>
  </servlet-mapping>

  <servlet-mapping>
    <servlet-name>UpdateHistoryServlet</servlet-name>
    <url-pattern>/js/updateHistory.js</url-pattern>
  </servlet-mapping>

  <servlet-mapping>
    <servlet-name>BookmarkServlet</servlet-name>
    <url-pattern>/js/bookmark.js</url-pattern>
  </servlet-mapping>

  <mime-mapping>
    <extension>js</extension>
    <mime-type>application/javascript;charset=UTF-8</mime-type>
  </mime-mapping>

  <welcome-file-list>
    <welcome-file>index.jsp</welcome-file>
  </welcome-file-list>


</web-app>

